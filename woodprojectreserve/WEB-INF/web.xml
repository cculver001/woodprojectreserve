<?xml version="1.0" encoding="UTF-8"?>

<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">

	<display-name>woodprojectreserve web app</display-name>
	
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>com.woodprojectreserve.controller.LoginController</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>reservation</servlet-name>
		<servlet-class>com.woodprojectreserve.controller.ReservationController</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>registration</servlet-name>
		<servlet-class>com.woodprojectreserve.controller.RegistrationController</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>home</servlet-name>
		<servlet-class>com.woodprojectreserve.view.HomeServlet</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>error</servlet-name>
		<servlet-class>com.woodprojectreserve.view.ErrorServlet</servlet-class>
	</servlet>
    
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>reservation</servlet-name>
		<url-pattern>/reservation</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>registration</servlet-name>
		<url-pattern>/registration</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>home</servlet-name>
		<url-pattern>/home</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>error</servlet-name>
		<url-pattern>/error</url-pattern>
	</servlet-mapping>

</web-app>
